{
  "vtex.menu@2.x:menu#websites": {
    "props": {"experimentalOptimizeRendering": false},
    "children": ["flex-layout.col#logo-desktop", "search-bar"]
  },
  "flex-layout.col#logo-desktop": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": ["logo#desktop"]
  },

  "search-bar": {
    "blocks": ["autocomplete-result-list.v2"],
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "openAutocompleteOnFocus": true,
      "text": ""
    }
  },

  "autocomplete-result-list.v2": {
    "props": {
      "__unstableProductOriginVtex": true
    },
    "blocks": ["product-summary.shelf"]
  },

  "vtex.menu@2.x:menu#institutional": {
    "props": {"experimentalOptimizeRendering": false},
    "children": [
      "menu-item#receita",
      "login",
      "rich-text#institutional",
      "minicart.v2"
    ]
  },

  "menu-item#receita": {
    "props": {
      "id": "menu-item-receita",
      "blockClass": "menu-top-receita",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/receita",
        "noFollow": false,
        "tagTitle": "Envie sua \n Receita",
        "text": "Envie sua \n Receita"
      }
    }
  },

  "menu-item#meuspedidos": {
    "props": {
      "id": "menu-item-meuspedidos",
      "blockClass": "menu-top-pedidos",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/account/orders",
        "noFollow": false,
        "tagTitle": "Meus \n Pedidos",
        "text": "Meus \n Pedidos"
      }
    }
  },

  "rich-text#institutional": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",

      "textColor": "#b19e8a",
      "blockClass": "help-message"
    }
  },

  "menu-item#minhaconta": {
    "props": {
      "id": "menu-item-shop",
      "blockClass": "menu-top-minha-conta",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/account#/profile",
        "noFollow": false,
        "tagTitle": "Minha Conta",
        "text": "Minha Conta"
      }
    }
  }
}
